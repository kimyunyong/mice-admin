		return code_name;
	}
	public void setCode_name(String code_name) {
		this.code_name = code_name;
	}	
	
	public String getCode_item() {
		return code_item;
	}
	public void setCode_item(String code_item) {
		this.code_item = code_item;
	}	
	
	
