@charset "utf-8";

/* 모바일 화면 css 작성 */
#loginContainer{
		position: relative;
		top: 0;
		left: 0;
		width: auto;
		margin: 0;
		padding-top: 8px;
}
#loginContainer > h1{
	height: 34.5px;
	background: url('/images/head_logo.png') no-repeat;
	background-size: contain;
	-webkit-background-size: contain;
}
.no-backgroundsize #loginContainer > h1{
	background: url('/images/head_logo_m.png') no-repeat;
}
.loginBox button{
	width: 100%;
}
