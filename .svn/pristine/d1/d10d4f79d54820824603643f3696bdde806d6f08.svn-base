/* **************************************** *
 * header
 * **************************************** */
#header{border-top:3px solid #060e15; border-bottom:1px solid #060e15;width: 100%; z-index:80;
overflow:hidden;background: #1f2f3c; padding:17px 20px; box-sizing:border-box;  position:fixed; top:0; left:0;}
	#header h1 { overflow:hidden; cursor:pointer; }









/* **************************************** *
 * gnb
 * **************************************** */
#leftGnb ul.gnb_sidebar > li > a{
	display: block;
	position: relative;
	padding: 13px 5px;
	font-size: 15px;
	font-weight: 600;
	color: #eee;
	border-bottom: 1px solid #414247; opacity:0.7;
}



#leftGnb h3 {color:#fff; font-size:19px; line-height:35px; margin-top:25px;}
#leftGnb h3 i{color:#fff; font-size:19px; line-height:35px;}




#leftGnb ul.gnb_sidebar > li.active{
	background-color: #1a1a1a;
}
#leftGnb ul.gnb_sidebar > li.active > a{
	font-weight: bold;
	color: #fff;
}
#leftGnb ul.gnb_sidebar > li > a > span.arrow{
	color: #414247;
}
#leftGnb ul.gnb_sidebar > li > a > span.arrow::before{
	content: '\f104';
	float: right;
	display: inline;
	margin: 0 5px 0 0;
	font-size: 16px;
	font-family: FontAwesome;
	height: auto;
	font-weight: 300;
	color: #acacac;
}
#leftGnb ul.gnb_sidebar > li.open > a > span.arrow::before{
	content: '\f107';
	float: right;
	display: inline;
	margin: 0 3px 0 0;
	font-size: 16px;
	font-family: FontAwesome;
	height: auto;
	font-weight: 300;
	color: #fff;
}
.lt-ie9 #leftGnb ul.gnb_sidebar > li > a > span.arrow{
	display: block;
	position: absolute;
	width: 5px;
	height: 10px;
	top: 50%;
	right: 15px;
	margin-top: -5px;
	background: url('/images/common_icon.png') no-repeat 0 -60px;
} /* IE8 이하 버전 수정 */
.lt-ie9 #leftGnb ul.gnb_sidebar > li.open > a > span.arrow{
	display: block;
	position: absolute;
	width: 10px;
	height: 5px;
	top: 50%;
	right: 12px;
	margin-top: -2.5px;
	background: url('/images/common_icon.png') no-repeat -5px -65px;
}

/* IE8 이하 버전 수정 */
.lt-ie9 #leftGnb ul.gnb_sidebar > li > a > span.arrow::before{
	content: '';
} /* IE8 이하 버전 수정 */
.lt-ie9 #leftGnb ul.gnb_sidebar > li.open > a > span.arrow::before{
	content: '';
} /* IE8 이하 버전 수정 */



#leftGnb ul.gnb_sidebar > li > ul.sub-menu{
	display: none;
	clear: both;
	margin: 8px 0;
}
#leftGnb ul.gnb_sidebar > li > ul.sub-menu > li > a{
	display: block;
	margin: 0;
	padding: 5px 0 5px 12px;
	font-size: 14px;
	color: #cecfd3;
}
#leftGnb ul.gnb_sidebar > li > ul.sub-menu > li.active > a{
	font-weight: bold;
	color: #fff;
}



#leftGnb ul.gnb_sidebar > li > ul.sub-menu li ul li a{
	display: block;
	margin: 0;
	padding: 4px 26px;
	font-size: 13px;
	color: #cecfd3;
}



