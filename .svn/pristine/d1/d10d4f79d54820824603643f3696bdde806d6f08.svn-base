.cb::after{content: ''; clear: both; float: none; margin: 0; padding: 0; font-size: 0; line-height: 1px; height: 1px; display: block;}
.inputBox_B{padding: 5px; border: 1px solid #dedede; border-radius: 5px; behavior: url('PIE.htc');}
.inputBox_B input{padding: 0; margin: 0; width: 100%; height: 24px; line-height: 24px; font-size: 16px; border: 0;}
label.hide{position: absolute; top: -9999px; font-size: 1px; line-height: 0; text-indent: -9999px; z-index: 1;}
select.selinp{padding: 0; margin: 0; height: 18px; line-height: 18px; border: 1px solid #d0cfcc;}
input.inp1{padding: 0; margin: 0; width: 98%; height: 18px; line-height: 18px; border: 1px solid #d0cfcc;}
