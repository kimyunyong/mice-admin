 .tooltip_area { background:#fff;  opacity:0.7;  box-shadow:3px 3px 10px #000;
background:#fff; position:absolute; top:-10px; left:5px; z-index:99; width:93%; box-sizing:border-box; padding:6px}
 .tooltip_area dt { font-size:11px !important; line-height:13px !important; color:#000 !important; display:inline;}
 .tooltip_area dd { display:inline-block;}
 .tooltip_area dd a{ background:#275979; color:#222; font-weight:bold; line-height:20px; display:inline;
font-size:11px !important; padding:0px 3px; color:#fff !important; font-size:11px !important;}









.btn_right_area {overflow:hidden; text-align:right; padding-bottom:10px; float:right;}
